                              <Insets right="50.0" />
                           </HBox.margin>
                        </Button>
                        <Button fx:id="cancelButton" mnemonicParsing="false" onAction="#handleCancelButton" prefHeight="25.0" prefWidth="53.0" style="-fx-background-color: #25274D; -fx-text-fill: white;" styleClass="registerButton" text="Cancel">
                           <HBox.margin>
                              <Insets />
                           </HBox.margin>
                        </Button>
                     </children>
                     <VBox.margin>
                        <Insets />
                     </VBox.margin>
                  </HBox>
               </children>
            </VBox>
         </children>
      </AnchorPane>
      <VBox fx:id="consVbox" layoutX="471.0" layoutY="417.0" prefHeight="187.0" prefWidth="207.0" style="-fx-background-color: #25274D; -fx-border-color: #14152B;" visible="false" />
