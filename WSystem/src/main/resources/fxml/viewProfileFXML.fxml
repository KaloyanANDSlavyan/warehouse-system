                           <padding>
                              <Insets bottom="15.0" />
                           </padding>
                        </Label>
                     </children>
                  </HBox>
                  <HBox alignment="BASELINE_CENTER" prefHeight="20.0" prefWidth="380.0">
                     <children>
                        <Label text="Username:" textFill="WHITE">
                           <font>
                              <Font name="Gulim" size="16.0" />
                           </font>
                           <HBox.margin>
                              <Insets right="50.0" />
                           </HBox.margin>
                        </Label>
                        <Label fx:id="usernameLabel" text="username" textFill="#25274d">
                           <font>
                              <Font name="Agency FB" size="22.0" />
                           </font>
                           <padding>
                              <Insets bottom="15.0" />
                           </padding>
                        </Label>
                     </children>
                  </HBox>
                  <HBox alignment="BASELINE_CENTER" prefHeight="20.0" prefWidth="380.0">
                     <children>
                        <Label text="Email:" textFill="WHITE">
                           <font>
                              <Font name="Gulim" size="16.0" />
                           </font>
                           <HBox.margin>
                              <Insets right="50.0" />
                           </HBox.margin>
                        </Label>
                        <Label fx:id="emailLabel" text="email" textFill="#25274d">
                           <font>
                              <Font name="Agency FB" size="22.0" />
                           </font>
                           <padding>
                              <Insets bottom="15.0" />
                           </padding>
                        </Label>
                     </children>
                  </HBox>
                  <HBox alignment="BASELINE_CENTER" prefHeight="20.0" prefWidth="380.0">
                     <children>
                        <Label text="Phone number:" textFill="WHITE">
                           <font>
                              <Font name="Gulim" size="16.0" />
