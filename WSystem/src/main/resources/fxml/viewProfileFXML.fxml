                        </Label>
                        <Label fx:id="phoneLabel" text="phoneNumber" textFill="#25274d">
                           <font>
                              <Font name="Agency FB" size="22.0" />
                           </font>
                           <padding>
                              <Insets bottom="15.0" />
                           </padding>
                        </Label>
                     </children>
                  </HBox>
               </children>
            </VBox>
            <VBox fx:id="editVbox" alignment="CENTER" layoutX="105.0" layoutY="110.0" prefHeight="329.0" prefWidth="441.0" styleClass="firstPane" stylesheets="@../styling/registrationScreenCSS.css" visible="false">
               <children>
                  <HBox alignment="BASELINE_CENTER" prefHeight="20.0" prefWidth="380.0">
                     <children>
                        <TextField fx:id="firstNameField" promptText="First name" styleClass="usernameField" stylesheets="@../styling/registrationScreenCSS.css" />
                     </children>
                     <VBox.margin>
                        <Insets bottom="20.0" />
                     </VBox.margin>
                  </HBox>
                  <HBox alignment="BASELINE_CENTER" prefHeight="20.0" prefWidth="380.0">
                     <children>
                        <TextField fx:id="lastNameField" promptText="Last name" styleClass="usernameField" />
                     </children>
                     <VBox.margin>
                        <Insets bottom="20.0" />
                     </VBox.margin>
                  </HBox>
                  <HBox alignment="BASELINE_CENTER" prefHeight="20.0" prefWidth="380.0">
                     <children>
                        <TextField fx:id="usernameField" promptText="Username" styleClass="usernameField">
