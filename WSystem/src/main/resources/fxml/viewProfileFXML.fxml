                           <HBox.margin>
                              <Insets right="50.0" />
                           </HBox.margin>
                        </Label>
                        <Label fx:id="firstNameLabel" text="firstName" textFill="#25274d">
                           <font>
                              <Font name="Agency FB" size="22.0" />
